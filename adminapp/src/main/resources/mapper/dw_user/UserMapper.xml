<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.adminapp.business.dao.dw_user.UserDao">
    <select id="login" resultType="com.adminapp.business.entity.dw_user.UserModel">
        select * from t_user where accountname=#{userName}
    </select>
    <select id="getUser" resultType="com.adminapp.business.entity.dw_user.UserModel">
        select * from t_user where id=#{id}
    </select>
</mapper>