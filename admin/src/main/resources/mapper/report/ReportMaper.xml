<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.admin.admin.dao.dw_report.ReportDao">

    <select id="getReport" resultType="com.admin.admin.entity.dw_report.Reportsettings">
        select * from t_reportsettings
    </select>

    <delete id="deleteReport" parameterType="int">
        delete * from t_reportsettings
    </delete>

    <insert id="SaveReport" parameterType="com.admin.admin.entity.dw_report.Reportsettings">
        INSERT INTO t_reportsettings(type,locationstime,reportcount,reporttype,reportday,begindate)
        VALUES(#{type},#{locationstime},#{reportcount},#{reporttype},#{reportday}#{begindate})
    </insert>

    <update id="UpdateReport" parameterType="com.admin.admin.entity.dw_report.Reportsettings">
        update t_reportsettings set type=#{type},locationstime=#{locationstime},reportcoun=#{reportcount},reporttype=#{reporttype},
        reportday=#{reportday},begindate=#{begindate} where id=#{id}
    </update>

</mapper>