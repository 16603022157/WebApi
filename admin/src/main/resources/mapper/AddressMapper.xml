<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.admin.admin.dao.person.AddressDao">

    <insert id="addLocation" parameterType="com.admin.admin.entity.person.AddressInformation">
        insert into t_location(latitude,longitude,address,person_id,timesta) VALUES (#{latitude},#{longitude},#{address},#{person_id},#{timesta})
    </insert>

</mapper>