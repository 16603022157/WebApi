<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.admin.admin.dao.dw_alarm.AlarmDao">

    <select id="getAlarm" resultType="com.admin.admin.entity.dw_alarm.Alarmsettings">
        select * from t_ alarmsettings
    </select>

    <delete id="deleteAlarm" parameterType="int">
        delete t_ alarmsettings where id=#{id}
    </delete>

    <update id="UpdateAlarm" parameterType="com.admin.admin.entity.dw_alarm.Alarmsettings">
        update t_ alarmsettings set code=#{code},alarmname=#{alarmname},alarmcount=#{alarmcount},continuous=#{continuous} where id=#{id}
    </update>
    <insert id="SaveAlarm" parameterType="com.admin.admin.entity.dw_alarm.Alarmsettings">
        INSERT INTO "t_ alarmsettings"(code,alarmname,alarmcount,continuous) VALUES(#{code},#{alarmname},#{alarmcount},#{continuous})
    </insert>

</mapper>